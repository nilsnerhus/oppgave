  shorttitle = {Decolonizing {{Climate Research}} and {{Policy}}},
  author = {Deranger, Eriel Tchekwie and Sinclair, Rebecca and Gray, Beze and McGregor, Deborah and Gobby, Jen},
  date = {2022-01-31},
  journaltitle = {Community Development Journal},
  volume = {57},
  number = {1},
