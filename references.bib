@article{klopp2019,
  title = {Mapping Minibuses in {{Maputo}} and {{Nairobi}}: Engaging Paratransit in Transportation Planning in {{African}} Cities},
  shorttitle = {Mapping Minibuses in {{Maputo}} and {{Nairobi}}},
