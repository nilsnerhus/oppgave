@article{almenar2021,
  title = {Nexus between Nature-Based Solutions, Ecosystem Services and Urban Challenges},
  author = {Almenar, Javier and Elliot, Thomas and Rugani, Benedetto and Philippe, Bod√©nan and Navarrete Gutierrez, Tomas and Sonnemann, Guido and Geneletti, Davide},
  date = {2021-01-01},
