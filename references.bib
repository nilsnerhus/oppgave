@article{andersson2016,
  title = {Europe's Failed ‚ÄòFight‚Äô against Irregular Migration: Ethnographic Notes on a Counterproductive Industry},
  shorttitle = {Europe's Failed ‚ÄòFight‚Äô against Irregular Migration},
  author = {Andersson, Ruben},
  date = {2016-05-27},
  journaltitle = {Journal of Ethnic and Migration Studies},
