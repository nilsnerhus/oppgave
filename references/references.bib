}

@article{dunlap2018,
  title = {The ‚Äòsolution‚Äô is now the ‚Äòproblem:‚Äô wind energy, colonisation and the ‚Äògenocide-ecocide nexus‚Äô in the Isthmus of Tehuantepec, Oaxaca},
  shorttitle = {The ‚Äòsolution‚Äô is now the ‚Äòproblem},
  author = {Dunlap, Alexander},
