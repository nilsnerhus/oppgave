@article{ahlqvist2015,
  title = {Emerging pathways for critical futures research: Changing contexts and impacts of social theory},
  shorttitle = {Emerging pathways for critical futures research},
  author = {Ahlqvist, Toni and Rhisiart, Martin},
  date = {2015-08-01},
  journaltitle = {Futures},
  shortjournal = {Futures},
  volume = {71},
